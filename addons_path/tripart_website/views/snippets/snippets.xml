<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template
        id="custom_snippets"
        inherit_id="website.snippets"
        name="Custom Snippets"
    >
        <xpath expr="//div[@id='snippet_structure']" position="before">
            <div id="snippet_structure" class="o_panel">
                <div class="o_panel_header">TipartWebsite</div>
                <div class="o_panel_body">
                    <t 
                        t-snippet="tripart_website.get-artist" >
                        <keywords>explore, artist</keywords>
                    </t>
                    <t 
                        t-snippet="tripart_website.get-artist-details" >
                        <keywords>know, artist</keywords>
                    </t>
                </div>
            </div>
        </xpath>
    </template>
</odoo>

<!-- inherit_id="website.snippets" -->