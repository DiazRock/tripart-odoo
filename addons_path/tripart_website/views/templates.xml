<odoo>
  <template id="index">
    <t t-call="website.layout">
      <t t-set="title">Tripart</t>
      <div class="oe_structure">
        <div class="container">
            <t t-foreach="artists" t-as="artist">
              <p>
                <a t-attf-href="/tripart/artist/{{ slug(artist) }}"/>
                <t t-esc="artist.id"/><t t-esc="artist.name"/>
              </p>
            </t>
        </div>
      </div>
    </t>
  </template>
  <template id="biography">
    <t t-call="website.layout">
        <t t-set="title">Tripart</t>
        <div class="oe_structure"/>
        <div class="oe_structure">
            <div class="container">
                <h3><t t-field="person.name"/></h3>
                <div><t t-field="person.biography"/></div>
            </div>
        </div>
        <div class="oe_structure"/>
    </t>
  </template>
</odoo>