<odoo>

    <!-- actions opening views on models -->

    <record model="ir.actions.act_window" id="tripart_website.action_window_artist">
      <field name="name">tripartWebsite window</field>
      <field name="res_model">tripart_website.artist</field>
      <field name="view_mode">tree,form</field>
    </record>

    <menuitem
        id="tripart_website_action_menu_artist"
        name="Artist"
        parent="tripart_website_menu"
        action="tripart_website.action_window_artist"
        sequence="10"
        />

    <record model="ir.actions.act_window" id="tripart_website.action_window_artwork">
      <field name="name">tripartWebsite window</field>
      <field name="res_model">tripart_website.artwork</field>
      <field name="view_mode">tree,form</field>
    </record>

    <menuitem
        id="tripart_website_action_menu_artwork"
        name="Artworks"
        parent="tripart_website_menu"
        action="tripart_website.action_window_artwork"
        sequence="10"
        />
</odoo>